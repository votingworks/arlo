{
  "name": "arlo-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "lint": "eslint --ext ts --ext tsx src",
    "lint:fix": "yarn lint --fix",
    "start": "react-scripts start",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:ci": "CI=true tsc && react-scripts test --maxWorkers 2 --env=jest-environment-jsdom-sixteen --coverage && yarn test:wdio",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:update": "react-scripts test -u  --watchAll=false",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:wdio": "FLASK_ENV=test pipenv run python ../app.py & FLASK_ENV=test pipenv run python ../bgcompute.py & react-scripts start & wdio wdio.conf.js"
  },
  "lint-staged": {
    "*.+(css|graphql|json|less|md|mdx|sass|scss|yaml|yml)": [
      "prettier --write"
    ],
    "*.+(js|jsx|ts|tsx)": [
      "stylelint",
      "eslint --quiet --fix"
    ],
    "*.+(ts|tsx)": [
      "bash -c tsc"
    ],
    "*.css": [
      "stylelint --config .stylelintrc-css.js --fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/contexts/ballotContext.ts",
      "!src/serviceWorker.ts",
      "!src/test/specs/*"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 79,
        "branches": 78,
        "lines": 79,
        "functions": 78
      }
    }
  },
  "dependencies": {
    "@blueprintjs/core": "^3.20.0",
    "@blueprintjs/table": "^3.8.1",
    "formik": "^1.5.8",
    "history": "^4.9.0",
    "http-proxy-middleware": "^0.19.1",
    "jspdf": "^1.5.3",
    "qrcode.react": "^0.9.3",
    "react": "16.13.1",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.3.1",
    "styled-components": "^4.3.2",
    "uuidv4": "^4.0.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/register": "^7.6.2",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^24.0.15",
    "@types/jsdom": "^12.2.4",
    "@types/jspdf": "^1.3.1",
    "@types/node": "12.0.7",
    "@types/qrcode.react": "^0.8.2",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-tap-event-plugin": "^0.0.30",
    "@types/react-toastify": "^4.0.2",
    "@types/styled-components": "^4.1.15",
    "@types/uuidv4": "^2.0.0",
    "@types/yup": "^0.26.22",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@wdio/cli": "^5.14.4",
    "@wdio/dot-reporter": "^5.14.4",
    "@wdio/jasmine-framework": "^5.14.4",
    "@wdio/local-runner": "^5.14.4",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.14.4",
    "babel-jest": "^24.9.0",
    "chromedriver": "83.0.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "is-ci-cli": "^1.1.1",
    "jest-canvas-mock": "^2.1.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-fetch-mock": "^2.1.2",
    "jest-styled-components": "^6.3.1",
    "jsdom": "^16.2.2",
    "lint-staged": "^10.0.8",
    "prettier": "^1.17.1",
    "react-scripts": "3.4.0",
    "react-test-renderer": "^16.13.1",
    "sort-package-json": "^1.40.0",
    "stylelint": "^10.0.1",
    "stylelint-config-palantir": "^4.0.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.7.0",
    "ts-node": "^8.4.1",
    "typescript": "3.5.1",
    "wdio-chromedriver-service": "^5.0.2"
  }
}
